{
  "name": "vue-spa-cli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --open --mode dev",
    "clean": "rimraf dist",
    "build": "npm run build:master",
    "build:dev": "npm run clean && vue-cli-service build --mode development",
    "build:test": "npm run clean && vue-cli-service build --mode test",
    "build:master": "npm run clean && vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "debug": "^4.1.1",
    "element-ui": "^2.7.2",
    "ramda": "^0.26.1",
    "vue": "^2.6.10",
    "vue-router": "^3.0.5",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/babel-preset-app": "^3.6.0",
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-vue": "^5.2.2",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
